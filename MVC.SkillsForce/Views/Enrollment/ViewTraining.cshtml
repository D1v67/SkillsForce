    .card {
        /*border: none;*/
        border-radius: 15px;
        transition: transform 0.3s ease-in-out;
        box-shadow: 0 8px 16px rgba(0, 0, 0, 0.3);
        margin-bottom: 40px;
    }

    .card-header {
        background-color: #3498db;
        color: #fff;
        padding: 10px;
